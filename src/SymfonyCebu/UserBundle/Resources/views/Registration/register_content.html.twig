{% trans_default_domain 'FOSUserBundle' %}

<h2 class="form-signin-heading">Register</h2>
{{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'class': 'fos_user_registration_register form-signin'}}) }}

    {{ form_widget(form) }}
    <input type="submit" id="_submit" name="_submit" value="{{ 'registration.submit'|trans }}" class="btn btn-lg btn-primary btn-block" />

{{ form_end(form) }}